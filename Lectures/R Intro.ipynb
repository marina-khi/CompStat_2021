   ],
   "source": [
    "y    <- c(1, 4, 5, 2, 7, 8, 2, 4)\n",
    "N    <- length(y)\n",
    "y.sq <- numeric(N)#creates an empty numeric vector of length N\n",
    "for(i in 1:N) { \n",
    "    y.sq[i] <- y[i]^2 #every element of y.sq is equal to the respective squared value of the element of y.\n",
    "    if (i == N) {     #only when this condition is met, i.e. only in the last iteration of the loop, we print y.sq\n",
    "        print(y.sq)\n",
    "    } else {          #in all of the other iterations of the loop, we print \"not finished\"\n",
    "        print(\"not finished\")\n",
    "    }\n",
    "}"
   ]
