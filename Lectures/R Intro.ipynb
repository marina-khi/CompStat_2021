    "LLN_Graph <- function(N, p) {\n",
    "    relative_share <- c()\n",
    "    for (n in 1:N) {\n",
    "        sample <- rbinom(n, 1, p)\n",
    "        relative_share <- c(relative_share, mean(sample))\n",
    "    }\n",
    "    plot(1:N, relative_share,\n",
    "         main = \"Law of large numbers\",\n",
    "         xlab = \"n\", ylab = \"Relative share\", pch = 16,\n",
    "         cex.axis = 1, cex.lab = 1, cex.main = 1)\n",
    "    abline(h = p, lwd = 2, col = 2)\n",
    "}\n",
    "\n",
    "LLN_Graph(N = 100, p = 0.4)\n",
    "LLN_Graph(N = 500, p = 0.4)\n",
    "LLN_Graph(N = 1000, p = 0.4)"
   ]
  },
