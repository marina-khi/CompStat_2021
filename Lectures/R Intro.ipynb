    "relative_share <- c() #Initialize an \"empty vector\" where we can store the results\n",
    "N <- 500              #Maximum sample size\n",
    "for (n in 1:N) {      #The loop itself: everything inside is repeated N times\n",
    "    sample <- rbinom(n, 1, 0.5) #draw a vector with sample size n\n",
    "    relative_share <- c(relative_share, mean(sample)) #calculate the proportion of the ones\n",
