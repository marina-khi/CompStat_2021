    "N  <- 10000 #Number of observations in the draw\n",
    "x  <- rnorm(N, mean = 0, sd = 1)\n",
    "z  <- seq(min(x), max(x), le = 100)\n",
    "hx <- dnorm(z)\n",
    "plot(density(x), main = \"\", ylim = c(0,0.45)) \n",
    "lines(z, hx, col=\"red\")"
